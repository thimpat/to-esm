{
  "name": "to-esm",
  "version": "1.6.4",
  "description": "Very simple tool to convert Commonjs files into ESM",
  "main": "tools/converter.cjs",
  "type": "commonjs",
  "bin": {
    "toesm": "./index.js",
    "to-esm": "./index.js"
  },
  "scripts": {
    "demo1": "toesm.cmd  --input=\"example/cjs/*.cjs\" --output=example/esm/",
    "demo2": "toesm.cmd  --input=\"example/cjs/**/*.?(c)js\" --output=example/esm/",
    "demo3": "toesm.cmd  --input=\"example/cjs/**/*.?(c)js\" --output=example/esm/ --config=\"example/.toesm.cjs\"",
    "demo4": "toesm.cmd  --input=\"example/cjs/**/*.?(c)js\" --output=example/browser/ --config=\"example/.toesm.cjs\" --solvedep",
    "demo": "npm run demo3 && npm run demo4",
    "test": "nyc mocha --sort"
  },
  "author": "Patrice Thimothee",
  "homepage": "https://github.com/thimpat/to-esm/blob/main/README.md",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thimpat/to-esm.git"
  },
  "dependencies": {
    "commondir": "^1.0.1",
    "esprima": "^4.0.1",
    "estraverse": "^5.3.0",
    "extract-comments": "^1.1.0",
    "glob": "^7.2.0",
    "minimist": "^1.2.5",
    "rgb-hex": "^4.0.0",
    "rgb-hex-cjs": "npm:rgb-hex@^3.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.0",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "chai": "^4.3.6",
    "mocha": "^9.2.0",
    "nyc": "^15.1.0",
    "semantic-release": "^19.0.2",
    "sinon": "^13.0.1"
  }
}
